<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Register</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="">
    </head>
    <body>
        <%- include('../partial/header.ejs') %>
        <%- include('../partial/navbar.ejs') %>
        <%- include('../partial/flash.ejs') %>   
        <div class="container" style="width: 500px; margin-top: 120px;" >
        <form  action="/register" method="POST" class="validated-form" novalidate>
            <h3>Register Page</h3>
            <div class="mb-3">
              <label for="exampleInputEmail1" class="form-label">Email address</label>
              <input type="email" name="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" required>
              </div>
            <div class="mb-3">
              <label for="exampleInputPassword1" class="form-label">Username</label>
              <input type="text"  name="username" class="form-control" id="exampleInputPassword1" required>
            </div>
            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label">Password</label>
                <input type="password" name="password" class="form-control" id="exampleInputPassword1" required>
              </div>
            
            <button type="submit" class="btn btn-primary">Register</button>
          </form>
          </div>
       
        
        <script src="" async defer></script>
    </body>
</html>